{
  "name": "ngx-dynamic-form-builder",
  "version": "1.6.0",
  "description": "FormBuilder + class-transformer + class-validator = dynamic form group builder for Angular10+",
  "keywords": [
    "form-builder",
    "class-transformer",
    "class-validator",
    "auto",
    "dynamic",
    "formgroup",
    "form",
    "validator",
    "validation",
    "object",
    "class",
    "transformer",
    "transform",
    "builder",
    "formarray",
    "angular",
    "angular10"
  ],
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=12",
    "npm": ">=6.4.0"
  },
  "bugs": {
    "url": "https://github.com/EndyKaufman/ngx-dynamic-form-builder/issues"
  },
  "homepage": "https://github.com/EndyKaufman/ngx-dynamic-form-builder",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EndyKaufman/ngx-dynamic-form-builder.git"
  },
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "config:mono": "rucken config -m dev",
    "config:prod": "rucken config -m prod",
    "prepare:mono": "rucken prepare -m dev",
    "prepare:prod": "rucken prepare -m prod",
    "lib:build-lib": "ng build ngx-dynamic-form-builder --prod",
    "lib:postbuild-lib": "./node_modules/.bin/cp-cli ./CHANGELOG.md ./dist/ngx-dynamic-form-builder/CHANGELOG.md",
    "lib:build": "run-s prepare:prod lib:build-lib lib:postbuild-lib",
    "app:prebuild": "ng build demo --prod",
    "app:build": "run-s format:fix prepare:mono app:prebuild",
    "lite-server": "lite-server -c bs-config.json",
    "ng": "ng",
    "serve": "ng serve",
    "start": "run-s format:fix prepare:mono serve",
    "start:prod": "run-s format:fix prepare:mono serve -- --prod",
    "build": "run-s app:build",
    "test": "run-s github:build",
    "lint": "ng lint",
    "tslint": "tslint",
    "e2e": "ng e2e",
    "github:prebuild": "ng build demo --base-href /ngx-dynamic-form-builder/ --prod",
    "github:build": "run-s format:fix lib:build config:prod github:prebuild config:mono lib:build github:postbuild",
    "github:postbuild": "./node_modules/.bin/cp-cli ./dist/demo/index.html ./dist/demo/404.html",
    "github:deploy": "gh-pages -d dist/demo",
    "format:fix": "prettier  --config ./.prettierrc --write \"{apps,libs}/**/*.ts\"",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.9",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^10.0.9",
    "@angular/compiler": "^10.0.9",
    "@angular/core": "^10.0.9",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^10.0.9",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "^10.0.9",
    "@angular/platform-browser-dynamic": "^10.0.9",
    "@angular/pwa": "^0.1000.6",
    "@angular/router": "^10.0.9",
    "@angular/service-worker": "^10.0.9",
    "@types/prismjs": "^1.16.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "core-js": "^3.6.5",
    "fast-safe-stringify": "^2.0.7",
    "lodash-es": "^4.17.15",
    "lodash.clonedeep": "^4.5.0",
    "lodash.mergewith": "^4.6.2",
    "ngx-markdown": "^10.1.0",
    "prismjs": "^1.21.0",
    "rxjs": "^6.6.2",
    "string-hash": "^1.1.3",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular-devkit/build-ng-packagr": "~0.1000.6",
    "@angular/cli": "^10.0.6",
    "@angular/compiler-cli": "^10.0.9",
    "@angular/language-service": "^10.0.9",
    "@rucken/cli": "^4.1.6",
    "@types/jasmine": "~3.5.12",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.mergewith": "^4.6.6",
    "@types/node": "~14.0.27",
    "@types/string-hash": "^1.1.1",
    "codelyzer": "~6.0.0",
    "conventional-changelog-cli": "^2.1.0",
    "conventional-commits-detector": "^1.0.3",
    "conventional-recommended-bump": "^6.0.10",
    "cp-cli": "^2.0.0",
    "gh-pages": "^3.1.0",
    "html-loader": "^1.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lite-server": "^2.5.4",
    "markdown-loader": "^5.1.0",
    "ng-packagr": "^10.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.1",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  }
}
