{
  "name": "ngx-dynamic-form-builder",
  "version": "0.4.3",
  "description": "FormBuilder + class-transformer + class-validator = dynamic form group builder for Angular6+",
  "keywords": [
    "FormBuilder",
    "class-transformer",
    "class-validator",
    "dynamic",
    "angular6"
  ],
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=10",
    "npm": ">=6.4.0"
  },
  "bugs": {
    "url": "https://github.com/EndyKaufman/ngx-dynamic-form-builder/issues"
  },
  "homepage": "https://github.com/EndyKaufman/ngx-dynamic-form-builder",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EndyKaufman/ngx-dynamic-form-builder.git"
  },
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "config:mono": "rucken config -m dev",
    "config:prod": "rucken config -m prod",
    "prepare:mono": "rucken prepare -m dev",
    "prepare:prod": "rucken prepare -m prod",
    "lib:build-lib": "ng build ngx-dynamic-form-builder --prod",
    "lib:build": "npm run prepare:prod && npm run lib:build-lib",
    "app:prebuild": "ng build demo --prod",
    "app:build": "run-s prepare:mono app:prebuild",
    "lite-server": "lite-server -c bs-config.json",
    "ng": "ng",
    "start": "npm run prepare:mono && ng serve",
    "build": "npm run app:build",
    "test": "npm run github:build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "github:prebuild": "ng build demo --base-href /ngx-dynamic-form-builder/ --prod",
    "github:build": "run-s lib:build config:prod github:prebuild github:postbuild",
    "github:postbuild": "./node_modules/.bin/cp-cli ./dist/demo/index.html ./dist/demo/404.html",
    "github:deploy": "gh-pages -d dist/demo"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.7",
    "@angular/compiler": "^6.1.7",
    "@angular/core": "^6.1.7",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/forms": "^6.1.7",
    "@angular/http": "^6.1.7",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.7",
    "@angular/platform-browser-dynamic": "^6.1.7",
    "@angular/pwa": "^0.8.2",
    "@angular/router": "^6.1.7",
    "@angular/service-worker": "^6.1.7",
    "@ngx-docs/example": "^0.1.6",
    "@ngx-prism/core": "^2.0.1",
    "@types/lodash-es": "^4.17.1",
    "@types/prismjs": "^1.9.0",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.9.1",
    "core-js": "^2.5.7",
    "immutable": "^3.8.2",
    "lodash-es": "^4.17.11",
    "prismjs": "^1.15.0",
    "rxjs": "^6.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.2",
    "@angular-devkit/build-ng-packagr": "~0.8.2",
    "@angular/cli": "^6.2.2",
    "@angular/compiler-cli": "^6.1.7",
    "@angular/language-service": "^6.1.7",
    "@rucken/cli": "^3.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.9.4",
    "codelyzer": "~4.4.4",
    "conventional-changelog-cli": "^2.0.5",
    "conventional-commits-detector": "^0.1.1",
    "conventional-recommended-bump": "^4.0.1",
    "cp-cli": "^1.1.2",
    "gh-pages": "^2.0.0",
    "html-loader": "^0.5.5",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "lite-server": "^2.4.0",
    "markdown-loader": "^4.0.0",
    "ng-packagr": "^4.1.1",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tsickle": "^0.32.1",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  }
}