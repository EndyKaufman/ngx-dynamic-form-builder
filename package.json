    "npm-run-all": "4.1.5",
    "nx": "16.2.1",
    "postcss": "8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "rucken": "^4.6.2",
    "standard-version": "9.5.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
