{
  "name": "ngx-dynamic-form-builder",
  "version": "1.11.0",
  "description": "FormBuilder + class-transformer + class-validator-multi-lang = dynamic form group builder for Angular10+",
  "keywords": [
    "form-builder",
    "class-transformer",
    "class-validator",
    "class-validator-multi-lang",
    "auto",
    "dynamic",
    "formgroup",
    "form",
    "validator",
    "validation",
    "object",
    "class",
    "transformer",
    "transform",
    "builder",
    "formarray",
    "angular",
    "angular10"
  ],
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=12",
    "npm": ">=6.4.0"
  },
  "bugs": {
    "url": "https://github.com/EndyKaufman/ngx-dynamic-form-builder/issues"
  },
  "homepage": "https://github.com/EndyKaufman/ngx-dynamic-form-builder",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EndyKaufman/ngx-dynamic-form-builder.git"
  },
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "config:mono": "rucken config -m dev",
    "config:prod": "rucken config -m prod",
    "prepare:mono": "rucken prepare -m dev",
    "prepare:prod": "rucken prepare -m prod",
    "lib:build-lib": "ng build ngx-dynamic-form-builder --prod",
    "lib:postbuild-lib": "./node_modules/.bin/cp-cli ./CHANGELOG.md ./dist/ngx-dynamic-form-builder/CHANGELOG.md",
    "lib:build": "run-s prepare:prod lib:build-lib lib:postbuild-lib",
    "app:prebuild": "ng build demo --prod",
    "app:build": "run-s format:fix prepare:mono app:prebuild",
    "lite-server": "lite-server -c bs-config.json",
    "ng": "ng",
    "serve": "ng serve",
    "start": "run-s format:fix i18n:extract prepare:mono serve",
    "start:prod": "run-s format:fix i18n:extract prepare:mono serve -- --prod",
    "build": "run-s app:build",
    "test": "run-s github:build",
    "lint": "ng lint",
    "tslint": "tslint",
    "e2e": "ng e2e",
    "github:prebuild": "ng build demo --base-href /ngx-dynamic-form-builder/ --prod",
    "github:build": "run-s format:fix i18n:extract lib:build config:prod github:prebuild config:mono lib:build github:postbuild",
    "github:postbuild": "./node_modules/.bin/cp-cli ./dist/demo/index.html ./dist/demo/404.html",
    "github:deploy": "gh-pages -d dist/demo",
    "format:fix": "tslint -q -p tsconfig.json -c ./tslint.json  --fix && prettier  --config ./.prettierrc --write \"{apps,libs}/**/*.ts\"",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "i18n:extract": "transloco-keys-manager extract"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.9",
    "@angular/cdk": "^11.0.4",
    "@angular/common": "^11.0.9",
    "@angular/compiler": "^11.0.9",
    "@angular/core": "^11.0.9",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.0.9",
    "@angular/material": "^11.0.4",
    "@angular/material-moment-adapter": "^11.0.4",
    "@angular/platform-browser": "^11.0.9",
    "@angular/platform-browser-dynamic": "^11.0.9",
    "@angular/pwa": "^0.1100.7",
    "@angular/router": "^11.0.9",
    "@angular/service-worker": "^11.0.9",
    "@ngneat/transloco": "^2.20.0",
    "@types/prismjs": "^1.16.2",
    "class-transformer": "^0.3.2",
    "class-validator-multi-lang": "^0.130.100",
    "core-js": "^3.6.5",
    "fast-safe-stringify": "^2.0.7",
    "lodash-es": "^4.17.20",
    "lodash.clonedeep": "^4.5.0",
    "lodash.mergewith": "^4.6.2",
    "ngx-markdown": "^11.0.1",
    "prismjs": "^1.23.0",
    "rxjs": "^6.6.3",
    "string-hash": "^1.1.3",
    "zone.js": "~0.10.3",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "^11.0.7",
    "@angular/compiler-cli": "^11.0.9",
    "@angular/language-service": "^11.0.9",
    "@ngneat/transloco-keys-manager": "^2.7.1",
    "@rucken/cli": "^4.1.6",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.mergewith": "^4.6.6",
    "@types/node": "~14.14.21",
    "@types/string-hash": "^1.1.1",
    "codelyzer": "~6.0.1",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-commits-detector": "^1.0.3",
    "conventional-recommended-bump": "^6.1.0",
    "cp-cli": "^2.0.0",
    "gh-pages": "^3.1.0",
    "html-loader": "^1.3.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lite-server": "^2.6.1",
    "markdown-loader": "^6.0.0",
    "ng-packagr": "^11.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.1",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.5"
  }
}
