    "@types/validator": ">=13.7.17",
    "class-transformer-global-storage": ">=0.4.1-1",
    "class-validator-multi-lang": ">=0.130.201",
    "codelyzer": "6.0.2",
    "cp-cli": "2.0.0",
    "gh-pages": "5.0.0",
    "html-loader": "4.2.0",
    "lodash.clonedeep": ">=4.5.0",
    "lodash.get": ">=4.4.2",
    "lodash.mergewith": ">=4.6.2",
    "lodash.set": ">=4.3.2",
    "markdown-loader": "8.0.0",
    "moment": "2.29.4",
    "ngx-dynamic-form-builder": "file:integrations/app/lib/ngx-dynamic-form-builder-0.0.0.tgz",
    "ngx-markdown": "16.0.0",
