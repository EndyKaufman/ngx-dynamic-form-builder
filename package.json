{
  "name": "ngx-dynamic-form-builder",
  "version": "2.2.2",
  "description": "FormBuilder + class-transformer-global-storage + class-validator-multi-lang = dynamic form group builder for Angular12+",
  "keywords": [
    "form-builder",
    "class-transformer",
    "class-transformer-global-storage",
    "class-validator",
    "class-validator-multi-lang",
    "auto",
    "dynamic",
    "formgroup",
    "form",
    "validator",
    "validation",
    "object",
    "class",
    "transformer",
    "transform",
    "builder",
    "formarray",
    "angular",
    "angular12"
  ],
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "engines": {
    "node": ">=15",
    "npm": ">=7"
  },
  "bugs": {
    "url": "https://github.com/EndyKaufman/ngx-dynamic-form-builder/issues"
  },
  "homepage": "https://github.com/EndyKaufman/ngx-dynamic-form-builder",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EndyKaufman/ngx-dynamic-form-builder.git"
  },
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "scripts": {
    "nx": "nx",
    "rucken": "rucken",
    "postinstall": "ngcc --properties es2015 browser module main && npx browserslist@latest --update-db",
    "lint": "npm run tsc:lint && nx workspace-lint && nx run-many --target=lint --all",
    "lint:fix": "npm run tsc:lint && nx workspace-lint --fix && nx run-many --target=lint --all --fix && nx format:write --all",
    "tsc:lint": "tsc --noEmit -p tsconfig.base.json",
    "lite-server": "lite-server -c bs-config.json",
    "prepare:code": "npm run rucken -- prepare",
    "app-debug:serve": "nx serve debug",
    "app:dev-serve": "run-s lint:fix i18n:extract prepare:code && nx serve",
    "app:prod-serve": "run-s lint:fix i18n:extract prepare:code app:build lite-server",
    "app:build-app": "nx build demo --prod",
    "app:build": "run-s prepare:code lint:fix app:build-app",
    "lib:build-lib": "nx build ngx-dynamic-form-builder --prod",
    "lib:build-changelog": "./node_modules/.bin/cp-cli ./CHANGELOG.md ./dist/libs/ngx-dynamic-form-builder/CHANGELOG.md",
    "lib:build": "run-s prepare:code lint:fix lib:build-lib lib:build-changelog",
    "i18n:extract": "DEBUG=* transloco-keys-manager extract",
    "i18n:find": "DEBUG=config,paths transloco-keys-manager find",
    "cy:run": "cypress run",
    "test:integrations-prepare": "sh ./integrations/scripts/prepare-application.sh",
    "test:integrations-serve": "npx --yes serve ./integrations/app/dist/app",
    "test:integrations-run": "npm run test:integrations-serve & npm run cy:run",
    "test": "npm run lib:build && npm run test:integrations-prepare && npm run test:integrations-run",
    "release": "standard-version",
    "publish": "npm publish ./dist/libs/ngx-dynamic-form-builder",
    "create-release": "run-s lib:build release && git push --follow-tags origin develop",
    "github:prebuild": "nx build demo --base-href /ngx-dynamic-form-builder/ --prod",
    "github:build": "run-s lint:fix i18n:extract lib:build github:prebuild prepare:code github:postbuild",
    "github:postbuild": "./node_modules/.bin/cp-cli ./dist/apps/demo/index.html ./dist/apps/demo/404.html",
    "github:deploy": "gh-pages -d dist/apps/demo"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.2",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "13.2.2",
    "@angular/compiler-cli": "13.2.2",
    "@angular/language-service": "13.2.2",
    "@nrwl/angular": "13.4.6",
    "@nrwl/cli": "13.4.6",
    "@nrwl/cypress": "13.4.6",
    "@nrwl/eslint-plugin-nx": "13.4.6",
    "@nrwl/jest": "13.4.6",
    "@nrwl/linter": "13.4.6",
    "@nrwl/tao": "13.4.6",
    "@nrwl/workspace": "13.4.6",
    "@types/jest": "27.0.2",
    "@types/lodash.clonedeep": "4.5.6",
    "@types/lodash.get": "4.4.6",
    "@types/lodash.mergewith": "4.6.6",
    "@types/lodash.set": "4.3.6",
    "@types/node": "17.0.10",
    "@types/validator": "13.7.1",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "class-validator-multi-lang": "0.130.201",
    "cypress": "9.3.1",
    "cypress-multi-reporters": "1.5.0",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "jest": "27.4.7",
    "jest-preset-angular": "11.1.0",
    "lite-server": "2.6.1",
    "lodash.clonedeep": "4.5.0",
    "lodash.mergewith": "4.6.2",
    "lodash.set": "4.3.2",
    "mochawesome": "7.0.1",
    "ng-packagr": "13.1.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.5",
    "postcss-import": "14.0.2",
    "postcss-preset-env": "7.2.3",
    "postcss-url": "10.1.3",
    "prettier": "2.5.1",
    "rucken": "3.4.0",
    "standard-version": "9.3.2",
    "ts-jest": "27.1.3",
    "typescript": "4.4.2",
    "validator": "13.7.0"
  },
  "dependencies": {
    "@angular/animations": "13.2.2",
    "@angular/cdk": "13.2.1",
    "@angular/common": "13.2.2",
    "@angular/compiler": "13.2.2",
    "@angular/core": "13.2.2",
    "@angular/flex-layout": "*",
    "@angular/forms": "13.2.2",
    "@angular/material": "13.2.1",
    "@angular/material-moment-adapter": "13.2.1",
    "@angular/platform-browser": "13.2.2",
    "@angular/platform-browser-dynamic": "13.2.2",
    "@angular/pwa": "13.2.2",
    "@angular/router": "13.2.2",
    "@angular/service-worker": "13.2.2",
    "@ngneat/transloco": "3.1.3",
    "@ngneat/transloco-keys-manager": "3.3.3",
    "@types/prismjs": "1.16.6",
    "class-transformer-global-storage": "0.4.1-1",
    "codelyzer": "6.0.2",
    "cp-cli": "2.0.0",
    "gh-pages": "3.2.3",
    "html-loader": "3.1.0",
    "lodash.get": "4.4.2",
    "markdown-loader": "8.0.0",
    "moment": "2.29.1",
    "ngx-markdown": "13.1.0",
    "prismjs": "1.26.0",
    "raw-loader": "4.0.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.4.0",
    "tslib": "2.0.0",
    "zone.js": "0.11.4"
  }
}
